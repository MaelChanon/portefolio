{
  "name": "portefolio-gql",
  "version": "1.0.94",
  "description": "Gql server for Strasbourg University",
  "repository": "https://github.com/MaelChanon/portefolio",
  "scripts": {
    "dev": "mkdir temp | true && ts-node-dev --respawn --transpile-only index.ts",
    "build": "rimraf build && tsc",
    "test": "cross-env NODE_ENV=test env-cmd -f .env_test node build/index.js",
    "start": "node build/index.js",
    "seed": "node prisma/seed.js",
    "update-db:test": "cross-env NODE_ENV=test env-cmd -f .env_test npx prisma db push --accept-data-loss",
    "update-db": "yarn run prisma migrate dev",
    "generate": "yarn run generate:prisma && yarn run generate:nexus",
    "generate:test": "cross-env NODE_ENV=test env-cmd -f .env_test yarn run generate:prisma && yarn run generate:nexus",
    "generate:prisma": "yarn run prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json schema.ts"
  },
  "keywords": [],
  "author": "Leo's",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "3.14.0",
    "@sentry/node": "7.39.0",
    "apollo-server-caching": "3.3.0",
    "apollo-server-core": "3.11.1",
    "apollo-server-express": "3.11.1",
    "aws-sdk": "2.1325.0",
    "axios": "^1.6.7",
    "cors": "2.8.5",
    "cross-env": "^7.0.3",
    "csvtojson": "^2.0.10",
    "cuid": "2.1.8",
    "dataloader": "2.2.2",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "4.18.2",
    "global-agent": "3.0.0",
    "graphql": "15.6.1",
    "graphql-iso-date": "3.6.1",
    "graphql-middleware": "^6.1.35",
    "graphql-postgres-subscriptions": "1.0.5",
    "graphql-shield": "^7.6.5",
    "graphql-upload": "12.0.0",
    "graphql-ws": "5.8.2",
    "iron-session": "^6.3.1",
    "jsonwebtoken": "^9.0.2",
    "libreoffice-convert": "1.6.0",
    "lodash": "^4.17.21",
    "nexus": "1.3.0",
    "nexus-plugin-prisma": "0.35.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "pg": "8.9.0",
    "sqlite3": "5.1.7",
    "subscriptions-transport-ws": "0.11.0",
    "ws": "8.12.1"
  },
  "devDependencies": {
    "@types/global-agent": "2.1.3",
    "@types/graphql-iso-date": "3.4.3",
    "@types/jsonwebtoken": "^9",
    "@types/pg": "8.6.6",
    "@types/ws": "8.5.4",
    "cross-var": "^1.1.0",
    "diagnose-endpoint": "1.1.0",
    "dotenv-cli": "^6.0.0",
    "nodemon": "2.0.20",
    "prettier": "2.8.4",
    "prisma": "3.14.0",
    "rimraf": "3.0.2",
    "ts-node": "10.9.2",
    "ts-node-dev": "1.1.8",
    "typescript": "4.9.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
